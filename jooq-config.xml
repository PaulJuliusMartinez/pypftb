<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<configuration>
  <generator>
    <database>
      <name>com.abcanthur.website.codegenhack.JPADatabase</name>
      <properties>
        <property>
          <key>packages</key>
          <value>com.abcanthur.website.models</value>
        </property>
      </properties>
    </database>
    <!-- Generation target -->
    <target>
      <packageName>com.abcanthur.website.codegen</packageName>
      <directory>src/main/java</directory>
    </target>
    <!--  Generation flags -->
    <generate>
      <pojos>true</pojos>
    </generate>
  </generator>
</configuration>
